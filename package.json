{
    "name": "@studip/vue-components",
    "version": "0.1.4",
    "description": "Vue components for Stud.IP",
    "author": "luniki",
    "license": "GPLv2",
    "main": "dist/studip-vue-components.cjs.js",
    "module": "dist/studip-vue-components.esm.js",
    "repository": {
        "type": "git",
        "url": "git@github.com:luniki/studip-vue-components.git"
    },
    "files": [
        "src/*",
        "dist/*"
    ],
    "keywords": [
        "vue",
        "components",
        "stud.ip"
    ],
    "scripts": {
        "storybook": "start-storybook -p 6006 -s ./assets",
        "build-storybook": "build-storybook",
        "build": "bili --bundle-node-modules --format esm --format cjs"
    },
    "bugs": {
        "url": "https://github.com/luniki/studip-vue-components/issues"
    },
    "homepage": "https://github.com/luniki/studip-vue-components#readme",
    "contributors": [],
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@storybook/addon-actions": "^5.1.9",
        "@storybook/addon-links": "^5.1.9",
        "@storybook/addons": "^5.1.9",
        "@storybook/vue": "^5.1.9",
        "babel-loader": "^8.0.6",
        "babel-preset-vue": "^2.0.2",
        "bili": "^4.8.1",
        "css-loader": "^3.1.0",
        "rollup-plugin-vue": "^6.0.0",
        "style-loader": "^0.23.1",
        "vue": "^2.6.10",
        "vue-loader": "^15.7.1",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "env": {
            "browser": true
        },
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module"
        }
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4
    }
}
